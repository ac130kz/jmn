package org.netbeans.modules.musician;

/**
 *
 * @author Geertjan Wielenga
 */
public class MusicianUtilities {
    
    private static int myUniId;
    
    /** Creates a new instance of MusicianUtilities */
    public MusicianUtilities() {
    }
    
    public static int getId(int uniId) {
        return myUniId;
    }
    
    public static void setId(int uniId) {
        myUniId = uniId;
    }
    
}
