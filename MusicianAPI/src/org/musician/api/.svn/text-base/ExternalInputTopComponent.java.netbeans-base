package org.musician.api;

import org.openide.windows.TopComponent;

/**
 *
 * @author Demo
 */
public abstract class ExternalInputTopComponent extends TopComponent {

    public abstract void programChange(int program);

    public abstract void setScoreTopComponent(TopComponent scoreTC);

    public abstract ExternalInputTopComponent findInstance();
    
    public abstract void noteOn(int knum);

    public abstract void noteOff(int knum);
    
}
